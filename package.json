{
  "name": "moodthemestudio",
  "displayName": "Mood based theme generator",
  "description": "",
  "publisher": "MoodThemeStudio",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:moodBasedThemeChanger.setTheme"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moodBasedThemeChanger.showFeatureMenu",
        "title": "Change Theme"
      }
    ]
  },
  "configuration": {
    "type": "object",
    "title": "Mood-Based Theme Changer",
    "properties": {
      "moodBasedThemeChanger.neutralTheme": {
        "type": "string",
        "default": "Default Dark Modern",
        "description": "Theme used in neutral mood."
      },
      "moodBasedThemeChanger.focusedTheme": {
        "type": "string",
        "default": "Default Dark+",
        "description": "Theme used in focused mood."
      },
      "moodBasedThemeChanger.tiredTheme": {
        "type": "string",
        "default": "Default Light Modern",
        "description": "Theme used in tired mood."
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
